<div id="creditAnalysisReport" class="hide">
    <h3>Credit Analysis Report</h3>
    <div>
        <div class="credit-rating-report bordered contentPadding">
            <h1>Where you Stand</h1>
            <span>Your Credit Rating Score</span>
            <span>AA</span>
            <span>On May 10, 2016</span>
        </div>
        <div class="credit-description bordered">
            <span>Description</span>
            <textarea readonly="readonly">Your long term rating scale denote expectations of very low default risk. You have indicated very strong capacity for payment of financial commitments.</textarea>
            <hr />
            <div class="btn-edit-save">
                <a><span class="glyphicon  glyphicon-edit"></span></a>
                <a><span class="glyphicon  glyphicon-save hide"></span></a>
            </div>
        </div>
    </div>
    <hr />
    <div>
        <div class="payment-history-report bordered contentPadding">
            <h5>Payment History</h5>
            <div><div>GOOD</div></div>
            <textarea readonly="readonly">Your long term rating scale denote expectations of very low default risk. You have indicated very strong capacity for payment of financial commitments.</textarea>
            <hr />
            <div class="btn-edit-save">
                <a><span class="glyphicon  glyphicon-edit"></span></a>
                <a><span class="glyphicon  glyphicon-save hide"></span></a>
            </div>
        </div>
        <div class="amount-debit-report bordered contentPadding">
            <h5>Amount of Debit</h5>
            <div><div>NORMAL</div></div>
            <textarea readonly="readonly">Your long term rating scale denote expectations of very low default risk. You have indicated very strong capacity for payment of financial commitments.</textarea>
            <hr />
            <div class="btn-edit-save">
                <a><span class="glyphicon  glyphicon-edit"></span></a>
                <a><span class="glyphicon  glyphicon-save hide"></span></a>
            </div>
        </div>
        <div class="length-credit-history bordered contentPadding">
            <h5>Length of Credit History</h5>
            <div><div>EXCELLENT</div></div>
            <textarea readonly="readonly">Your long term rating scale denote expectations of very low default risk. You have indicated very strong capacity for payment of financial commitments.</textarea>
            <hr />
            <div class="btn-edit-save">
                <a><span class="glyphicon  glyphicon-edit"></span></a>
                <a><span class="glyphicon  glyphicon-save hide"></span></a>
            </div>
        </div>
    </div>
    <div>
        @Html.TextArea("comments", "Additional comments here", 3, 116, null)
        <div class="btn-edit-save">
            <a><span class="glyphicon  glyphicon-edit"></span></a>
            <a><span class="glyphicon  glyphicon-save hide"></span></a>
        </div>
    </div>
</div>